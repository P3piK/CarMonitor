<Page x:Class="CarMonitor.Client.Pages.CreateProfilePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CarMonitor.Client.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CreateProfilePage">

    <Grid>
        <TextBox x:Name="nameBox" HorizontalAlignment="Left" Height="22" Margin="156,152,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="160"/>
        <Button x:Name="createButton" Content="Create" HorizontalAlignment="Left" Margin="414,274,0,0" VerticalAlignment="Top" Width="75" Click="createButton_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="540,152,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" IsEnabled="False"/>

    </Grid>
</Page>
